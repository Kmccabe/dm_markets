def prepend_str(prepend_int):
    bs_str = str(prepend_int)
    if len(bs_str) == 1:
        r_str = "000" + bs_str
    elif len(bs_str) == 2:
        r_str = "00" + bs_str
    elif len(bs_str) == 3:
        r_str = "0" + bs_str
    else:
        r_str = bs_str
        
    return r_str


